# Project variables, overrides defaults in variables.tf

# Name of the GCP cloud project
# project = ""
# alternatively provide env var via "export TF_VAR_project=<name of GCP project>"


# Path to GCP credential file associated with user/project
# credential_file = ""

# alternatively provide env var via "export TF_VAR_credentials_file=<path to file>"

# Name of the GCP cloud project user
# project_username = ""

# Path to private key file for project user
# project_user_privkey_file = ""
